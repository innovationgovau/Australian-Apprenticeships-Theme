name = Australian Apprenticeships
description = Theme for the Australian Apprenticeships site.
base theme = omega
screenshot = screenshot.png
engine = phptemplate
core = 7.x

; ========================================
; Stylesheets
; ========================================
stylesheets[all][] = css/australian-apprenticeships.normalize.css
stylesheets[all][] = css/australian-apprenticeships.hacks.css
stylesheets[all][] = css/australian-apprenticeships.styles.css
