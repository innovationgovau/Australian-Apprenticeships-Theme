; ========================================
; Scripts
; ========================================
scripts[] = js/australian-apprenticeships.behaviors.js

; ========================================
; Regions
; ========================================
regions[branding] = Branding
regions[search] = Search
regions[top_menu] = Top Menu
regions[content] = Content
regions[social] = Social
regions[links] = Links
regions[footer] = Footer

; ========================================
; Plugins
; ========================================
